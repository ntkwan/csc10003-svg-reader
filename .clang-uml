compilation_database_dir: build
output_directory: docs/diagrams
generate_method_arguments: none
diagrams:
  Viewer:
    type: class
    include_relations_also_as_members: true
    include:
      elements:
        - Viewer
  Parser:
    type: class
    include_relations_also_as_members: true
    include:
      elements:
        - Parser
  Shape:
    type: class
    include_relations_also_as_members: true
    include:
      elements:
        - Shape
        - Circle
        - Polygon
        - Rectangle
        - Ellipse
        - Text
        - Line
        - Polyline
        - Rect
